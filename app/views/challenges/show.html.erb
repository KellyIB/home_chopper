<center>
  <main class="game-main">
    <h1>Here Are Your Game Results!!</h1>
    <%= "#{@challenge.user}" %>
    <h2>This was a <%= @challenge.time_limit %> minute challenge.</h2>
    <h2><%= "#{@challenge.user}" %> was challenged to make a <%= @challenge.meal_type %>.</h2>
    <h2>Their Mystery Basket Ingredients Were:</h2>
    <% @challenge.ingredients.each do |ingredient|%>
        <section id=final_ingredient-"<%= ingredient.id %>">
          <h3><%= ingredient.name %></h3>
        </section>
    <% end %>
  </main>
</center>
