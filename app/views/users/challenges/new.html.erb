<h2>New Challenge</h2>
  <%= form_for @challenge, url: "/users/#{current_user.id}/challenges", method: :post do |f| %>
  <%= f.select :time_limit, [['Three Ingredients','three'],['Five Ingredients','five']] %>
  <%= f.select :basket_size, [['Three Ingredients','three'],['Five Ingredients','five']] %>
  <%= f.text_field :meal_type  %>
  <%= f.submit "Start New Game"  %>
<% end %>
